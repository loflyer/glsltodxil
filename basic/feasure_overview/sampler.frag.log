#version 450 core

//  纹理采样器绑定在0      
layout(binding=0) uniform sampler1D tex0;

uniform int index_u;
out     vec4 result;

void main()
{
    float coord = 2.0;
    // 根据采样值和坐标 生成一个纹理值
    result = texture(tex0, coord);
}


#version 450
uniform int index_u;

layout (location = 0) in  vec4 inData;
layout (location = 0) out vec4 outData;

void main()
{
    gl_Position    = vec4(1.0f, 1.0f, 1.0f, float(index_u));
    outData.x = index_u + 1;
    outData.yzw = inData.xyz ; 
}
